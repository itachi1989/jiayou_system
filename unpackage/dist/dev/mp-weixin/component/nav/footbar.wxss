.container {
  width: 100vw;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  border-top: 1rpx solid #c0c0c0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  z-index: 999;
}
.container .list {
  height: 100%;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 24rpx;
  color: #c0c0c0;
}
.container .list image {
  margin-bottom: 10rpx;
  width: 40rpx;
  height: 40rpx;
}

